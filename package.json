{
  "name": "Trending-Repositories-Tracker",
  "displayName": "Trending Repositories Tracker",
  "description": "Repo Trend Analyzer â€“ Track trending repositories with real-time insights on stars, forks, commits, and growth predictions. Discover top repos, bookmark favorites, and explore contribution opportunitiesâ€”all within VS Code! ðŸš€",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:Trending-Repositories-Tracker.openWidget",
