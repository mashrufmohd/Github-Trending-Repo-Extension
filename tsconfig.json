{
	"compilerOptions": {
	  "module": "CommonJS",
	  "target": "ES2020",
	  "lib": ["ES2020", "DOM"],
	  "sourceMap": true,
	  "rootDir": "src",
	  "outDir": "dist",
	  "strict": true,
	  "jsx": "react-jsx",
	  "esModuleInterop": true,
	  "allowSyntheticDefaultImports": true,
	  "noImplicitAny": true,
	  "moduleResolution": "node"
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist"]
  }